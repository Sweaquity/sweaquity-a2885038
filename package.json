    "diagnostics:all": "npm run diagnostics",
    "diagnostics:view": "cat project-health/diagnostics/outputs/diagnostic-summary.md",
    "diagnostics:clean": "rm -rf project-health/diagnostics/outputs/* && echo 'Diagnostic outputs cleaned'",
    "automation:test": "npm run diagnostics:comprehensive && npm run diagnostics:nda && npm run diagnostics:functions",
    "test:e2e-automation": "node project-health/diagnostics/queries/e2e-automation-test.js",
    "test:automation-complete": "npm run diagnostics:comprehensive && npm run test:e2e-automation",
